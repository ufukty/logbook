    "WORKSPACE": "${workspaceFolder}"
  },
  "go.testTimeout": "30s",
  "prettier.printWidth": 120,
  "prettier.tabWidth": 4,
  "python.defaultInterpreterPath": ".venv/bin/python3",
  "window.title": "Logbook"
}