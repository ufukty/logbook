        "test",
        "-timeout",
        "10s",
        "-run",
        "^TestMigration$",
        "logbook/cmd/objectives/database",
        "-v",
        "-count",
        "1",
      ],
      "options": {
        "cwd": "${workspaceFolder}/app",
        "env": {
          "WORKSPACE": "${workspaceFolder}"
        }
      },
    }
  ]
}